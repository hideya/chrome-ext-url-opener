{
  "manifest_version": 3,
  "name": "URL Opener",
  "version": "1.0",
  "description": "Opens a new browser window when the extension icon is clicked",
  "action": {
    "default_title": "Open New Window"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ]
}
